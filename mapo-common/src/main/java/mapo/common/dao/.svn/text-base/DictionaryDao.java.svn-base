package mapo.common.dao;

/**
 * @author jmayaalv
 *
 */
public interface DictionaryDao {
	
	/**
	 * Adds a new entry to the dictionary table
	 * @param key table key
	 * @param value value translation
	 * @author jmayaalv
	 */
	public void write(int key, String value);

	/**
	 * Reads an entry from the dictionary
	 * @param keyCode
	 * @return
	 * @author jmayaalv
	 */
	public String get(int keyCode);

}
